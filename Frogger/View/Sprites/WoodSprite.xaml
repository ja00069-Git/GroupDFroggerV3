<local:BaseSprite
    x:Class="Frogger.View.Sprites.WoodSprite"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Frogger.View.Sprites"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Height="50"
    Width="100">

    <Canvas>
        <Rectangle Fill="SaddleBrown"  Height="50" Width="100" />
        <Rectangle Height="50" Stroke="Black" Width="100"/>
        <Rectangle Height="2" Fill="Black" Canvas.Top="5" Width="100"/>
        <Rectangle Height="2" Fill="Black" Canvas.Top="15" Width="100"/>
        <Rectangle Height="2" Fill="Black" Canvas.Top="25" Width="100"/>
        <Rectangle Height="2" Fill="Black" Canvas.Top="33" Width="100"/>
        <Rectangle Height="2" Fill="Black" Canvas.Top="42" Width="100"/>
    </Canvas>
</local:BaseSprite>